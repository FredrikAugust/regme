{
  "name": "regme",
  "version": "0.0.1",
  "description": "App for self-registration at events.",
  "scripts": {
    "start": "python -m SimpleHTTPServer",
    "build": "echo \"Building RegMe...\" && npm run build-js && npm run build-less",
    "deploy": "echo \"Deployed\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch-js & npm run watch-less",
    "build-js": "cat ./src/main.js | ./node_modules/uglifyjs/bin/uglifyjs -o ./src/regme.min.js",
    "build-less": "./node_modules/less/bin/lessc ./less/regme.less | ./node_modules/clean-css/bin/cleancss -d -o ./css/regme.min.css",
    "watch-js": "./node_modules/nodemon/bin/nodemon.js -e js -w ./js -x 'npm run build-js'",
    "watch-less": "./node_modules/nodemon/bin/nodemon.js -e less -w ./less -x 'npm run build-less'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotkom/regme.git"
  },
  "keywords": [
    "Event",
    "Registration",
    "App",
    "RFID"
  ],
  "author": {
    "name": "dotKom",
    "email": "dotkom@online.ntnu.no",
    "url": "https://online.ntnu.no"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotkom/regme/issues"
  },
  "homepage": "https://github.com/dotkom/regme#readme",
  "dependencies": {
    "clean-css": "^3.3.5",
    "less": "^2.5.1",
    "nodemon": "^1.3.7",
    "uglifyjs": "^2.4.10",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "~4.4.1",
    "redux": "^3.3.1"
  }
}